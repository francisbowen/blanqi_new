      {% assign name = "returns" %}
      <div class="unit navbar navbar-expand-sm">
        <h2 id="h2{{name}}" class="collapsed" data-toggle="collapse" data-target="#Target-{{name}}" aria-expanded="true" aria-controls="Target-{{name}}">{{name}}</h2>
