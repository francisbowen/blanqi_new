<!--  Begin holiday shipping banner    -->
<style>
  /*    new    */
  #HolidayShippingBanner {
    z-index: 2147483647;
    width: 700px;
    height: 400px;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: #fcab8f;
    text-align: left;
    color: white;
    margin: auto;
    opacity: 0;
    -webkit-transform: translateY(40px);
    -ms-transform: translateY(40px);
    transform: translateY(40px);
    -webkit-transition: all 300ms ease-in-out 0s;
    -o-transition: all 300ms ease-in-out 0s;
    transition: all 300ms ease-in-out 0s;
    pointer-events: none;
  }
  #HolidayShippingBanner.opened {
    -webkit-transform: translateY(0px);
    -ms-transform: translateY(0px);
    transform: translateY(0px);
    opacity: 1;
    pointer-events: all;
  }
  #HolidayShippingBanner .close-popup {
    background: url(data:image/png;base64,);
    width: 15px;
    height: 15px;
    position: absolute;
    top: 0;
    right: 0;
    background-size: contain;
    background-repeat: no-repeat;
    cursor: pointer;
    margin-right: 7px;
    margin-top: 6px;
  }
  #HolidayShippingBanner .hsb-left {
    display: inline-block;
    width: 50%;
    vertical-align: top;
    padding-top: 50px;
    text-align: center;
  }
  #HolidayShippingBanner h1,
  #HolidayShippingBanner h2
  {
    color: white;
    font-weight: 900;
    font-size: 5.1em;
    text-align: center;
    margin: 0;
    line-height: 1em;
    display:inline-block;
    vertical-align: top;
  }
  #HolidayShippingBanner h1.s40 {
    letter-spacing: 0.2em;
  }
  #HolidayShippingBanner h1.percent {
    font-weight: 400;
  }
  #HolidayShippingBanner h2 {
    letter-spacing: 0.2em;
    font-weight: 500;
  }
  #HolidayShippingBanner h3,
  #HolidayShippingBanner h4 {
    text-align: center;
    color: white;
    font-size: 1.1em;
    letter-spacing: 0.3em;
    line-height: 1.3em;
    margin: 0;
    padding: 0;
    font-weight: 600;
  }
  #HolidayShippingBanner h3 {
    letter-spacing: 0.36em;
    margin-top: 5px;
  }
  #HolidayShippingBanner a {
    font-weight: 900;
    color: white;
    letter-spacing: 0.3em;
    font-size: 0.8em;
    border: 1px solid white;
    padding: 0.5em 0;
    width: 204px;
    display: inline-block;
    margin-top: 38px;
    -webkit-transition: all 200ms ease-in-out 0s;
    -o-transition: all 200ms ease-in-out 0s;
    transition: all 200ms ease-in-out 0s;
  }
  #HolidayShippingBanner a:hover{
    background: white;
    color: black;
  }
  #HolidayShippingBanner img {
    width: 320px;
    height: 350px;
    position: absolute;
    right: 40px;
    top: 25px;
  }
  @media (max-width: 768px) {
    #HolidayShippingBanner .hsb-right {
      display: none;
    }
    #HolidayShippingBanner {
      width: 350px;
      height: 350px;
      max-width: 90%;
    }
    #HolidayShippingBanner .hsb-left {
      width: 100%;
      padding-top: 30px;
    }
    #HolidayShippingBanner .close-popup {
      margin-right: 10px;
      margin-top: 10px;
      width: 25px;
      height: 25px;
    }
  }
</style>
<div id="HolidayShippingBanner">
  <div id="CloseShippingPopup" class="close-popup">
  </div>
  <div class="hsb-left">
    <h1 class="s40">40</h1><h1 class="percent">%</h1>
    <h2>OFF</h2>
    <h3>POSTPARTUM +</h3>
    <h4>NURSING TANKS</h4>
    <a id="HSBShopSaleNow" href="https://www.blanqi.com/collections/nursing">SHOP SALE</a>
  </div>
  <div class="hsb-right">
    <img src="https://cdn.shopify.com/s/files/1/0169/0840/files/blanqi-jan2018-popup.jpg">
  </div>
  <script>
    $(document).ready(function()
                      {
      setTimeout(function()
                 {
        var was_closed = localStorage.getItem("HSB-close");
        if (was_closed && Date.now() - was_closed < (60000 * 30) * 48) // half hour * 48 = 24 hours
        {  
        }
        else
        {
          $("#HolidayShippingBanner").addClass("opened");
        }
      }, 3000);	

      $("#CloseShippingPopup").on("click",function(e)
                                  {
        document.getElementById('HolidayShippingBanner').className='closed';
        localStorage.setItem("HSB-close", Date.now() );
      });

      $("#HSBShopSaleNow").on("click",function(e)
                              {
        document.getElementById('HolidayShippingBanner').className='closed';
        localStorage.setItem("HSB-close", Date.now() );
      });
    });
  </script>
</div>
<!--   End holiday shipping banner   -->