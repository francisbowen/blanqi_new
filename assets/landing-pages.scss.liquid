/*  Media queries  */
{% assign mediaXL_up    = '(min-width: 1601px)' %}
{% assign mediaLG_only  = '(max-width: 1600px) and (min-width: 1301px)' %}
{% assign mediaMD_only  = '(max-width: 1300px) and (min-width: 1026px )' %}
{% assign mediaSM_only  = '(max-width: 1025px) and (min-width: 601px )' %}
{% assign mediaXS_only  = '(max-width: 768px)' %}
{% assign media_desktop = '(min-width: 769px)' %}
{% assign media_mobile  = '(max-width: 768px)' %}
{% assign media_xs      = '(max-width: 320px)' %}
{% assign media_retina  = '(-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi)' %}
@media {{media_mobile}} {
  .desktop_only {
    display: none !important;
  }
}
@media {{media_desktop}} {
  .mobile_only {
    display: none !important;
  }
}
.blanqi-styles-wrap /*  The wrapping element  */
{
  overflow: hidden;
  font-size: 13px;
  @media (max-width: 768px)  { font-size: 13px; }
  @media (max-width: 320px)  { font-size: 11px; }
  @media (min-width: 769px)  { font-size: 14px; }
  @media (min-width: 1441px) { font-size: 15px; }
  @media (min-width: 1701px) { font-size: 16px; }
  @media (min-width: 1941px) { font-size: 17px; }
  @media (min-width: 2000px) { font-size: 18px; }
  h1,h2,h3,p {
    margin: 0;
    padding: 0;
  }
  .btn-black {
    background: black;
    font-weight: 500;
    color: white;
    &.inverted {
      color: black;
      background: white;
    }
    padding: 0 4em;
    font-size: 1em;
    letter-spacing: 0.25em;
    line-height: 4em;
    display: inline-block;
    margin-bottom: 1.25em;
    text-transform: uppercase;
  }
  .btn-underline {
    color: black;
    border-bottom: 1px solid black;
    &.inverted {
      color: white;
      border-bottom: 1px solid white;
    }
    display: inline-block;
    font-size: 1em;
    padding-bottom: 0.4em;
    line-height: 1em;
    font-weight: 500;
    letter-spacing: 0.15em;
    text-transform: uppercase;
  }
  .btn-arrow {
    width: 11em;
    display: inline-block;
    text-align: right;
    font-weight: 500;
    color: black;
    font-size: 0.7em;
    letter-spacing: 0.2em;
    line-height: 2em;
    padding-right: 1.3em;
    margin-right: 1em;
    cursor: pointer;
    text-transform: uppercase;
    &:hover {
      color: black;
      text-decoration: none;
    }
    &:after {
      content: " ";
      height: 0.6em;
      width: 0.6em;
      border: 0.1em solid black;
      border-left: 0;
      border-bottom: 0;
      position: absolute;
      right: 0.4em;
      top: 0;
      -webkit-transform: rotateZ(45deg);
      -moz-transform:    rotateZ(45deg);
      -ms-transform:     rotateZ(45deg);
      -o-transform:      rotateZ(45deg);
      transform:         rotateZ(45deg);
      bottom: 0;
      margin: auto;
    }
  }
  .btn-arrow + .btn-arrow {
    margin-right: 3em;
  }
  .btn-transparent  {
    border: 1px solid black;
    padding: 0 3.9em;
    font-weight: 500;
    line-height: 4.3em;
    font-size: 0.7em;
    @media {{media_mobile}} {
      font-size: 0.8em;
    }
    display: inline-block;
    color: black;
    letter-spacing: 0.2em;
    transition: all 300ms ease-in-out 0s;
    text-transform: uppercase;
    &:hover {
      color: white;
      background: black;
    }
    &.inverted {
      color: white;
      border-color: white;
      &:hover {
        color: black;
        background: white;
      }
    }
    @media {{media_mobile}} {
      &.mobile-inverted {
        color: white;
        border-color: white;
        &:hover {
          color: black;
          background: white;
        }
      }
    }
  }
  .btn-large {
    width: 100%;
    line-height: 5.5em;
    font-size: 1em;
    @media {{media_mobile}} {
      font-size: 0.8em;
    }
  }
  b {
    font-weight: 900;
    letter-spacing: 0.15em;
  }
  .center-bottom {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 5%;
  }
  p.preheader {
    color: #848484;
    @media {{media_mobile}} {
      color: #545454;
    }
    margin-bottom: 0.8em;
    font-size: 0.9em;
    letter-spacing: 0.25em;
  }
  p.preheader2 {
    color: #848484;
    margin-bottom: 0.65em;
    font-size: 0.8em;
    font-weight: 500;
    letter-spacing: 0.22em;
    &.color-darkgrey {
      color: #6d6d6d;
    }
    &.color-black {
      color: #111;
    }
    @media {{media_mobile}} {
      &.mobile-color-darkgrey {
        color: #6d6d6d;
      }
      &.mobile-color-black {
        color: #111;
      }
    }
  }
  p.block-text2 {
    color: #8c8c8c;
    font-size: 0.85em;
    margin-top: 1.9em;
    line-height: 1.9em;
    width: 37em;
    letter-spacing: 0.04em;
    &.color-darkgrey {
      color: #6d6d6d;
    }
    &.color-black {
      color: #111;
    }
    @media {{media_mobile}} {
      &.mobile-color-darkgrey {
        color: #6d6d6d;
      }
      &.mobile-color-black {
        color: #111;
      }
    }
  }
  p.block-text2.top-margin {
    @media {{media_desktop}} {
      margin-top: 1.9em;
    }
  }
  &.our-story {
    /*
      Our Story page only
    */
    text-align: center;
    h1 {
      font-size: 1.6em;
      line-height: 1.2em;
      letter-spacing: 0.23em;
      margin: 0 auto 0.9em;
    }
    p {
      font-size: 0.7em;
      line-height: 2em;
      color: #000;
      letter-spacing: 0.1em;
      margin: 0 auto;
      &.preheader {
        margin: 0 auto 1.4em auto;
