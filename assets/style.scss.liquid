.center-xy {
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin: auto;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
}

@media (min-width: 769px) {
  #MainMenu .menu.align_left > li > a {
    background: white;
    padding: 0.2rem 0.4rem 0 0.5rem;
  }
}
.homepage-slideshow:not(.flickity-enabled) .gallery-cell {
  display: none;
  &:first-child {
    display: block;
  }
}
.homepage-slideshow, 
.main-carousel {
  .flickity-page-dots {
    display:none;
    margin-bottom: 1rem;
    .dot {
      width: 0.5rem;
      height: 0.5rem;
      border: 1px solid white;
      background: transparent;
      opacity: 1;
      margin: 0 0.5rem;
      border-radius: 50%;
      &.is-selected {
        background: white;
      }
    }
  }
  .flickity-prev-next-button:hover {
    background: transparent;
  }
}
.homepage-slideshow {
  .gallery-cell {
    background-color: #ccc;
  }
  .caption-content {
    -webkit-transition: all 300ms ease-in-out 0s, opacity 600ms ease-in-out 750ms;
    -o-transition: all 300ms ease-in-out 0s, opacity 600ms ease-in-out 750ms;
    transition: all 300ms ease-in-out 0s, opacity 600ms ease-in-out 750ms;
  }
  .is-selected {
    .caption-content {
      opacity: 1;
    }
  }
  .caption {
    @media (max-width: $breakpoint-md) {
      text-align: center;
    }
    .pre-head {
      font-size: 0.7rem;
      letter-spacing: 0.15rem;
      line-height: 1;
      margin: auto;
      padding: 0;
      font-weight: 700;
      @media (min-width: 1900px) {
        font-size: 1rem;
      }
    


    }
    .headline {
      font-size: 3rem;
      letter-spacing: 0.2rem;
      line-height: 1;
      margin: 0;
      padding: 0;
      font-weight: 700;
      @media (min-width: $breakpoint-md) {
        font-size: 5rem;
      }
    }
    .post-head {
      font-size: 1rem;
      letter-spacing: 0rem;
      margin: 0;
      line-height: 1.3;
    }

    .black-friday-caption {
      display: flex;
      flex-direction: column;
      justify-content: space-around;
      padding: 2em;
      
      @media(min-width: 1900px) {
        padding: 3em;
      }
      
      .pre-head {
        font-size: 0.55em;
        font-weight: 300;
        letter-spacing: 0.125em;
         @media (min-width: 1900px) {
          font-size: .72rem;
          margin-bottom:8px;
     	 }
          @media (max-width: 600px) {
            font-size: .58rem;
          }  
      }
      
      .post-head {
        font-size: .8em;
        
          @media(min-width: 1900px) {
            font-size: 1em;
          }        
        
          @media(max-width: 1150px) {
            font-size: 0.8em;
          }
        
         @media(max-width: 768px) {
            font-size: 0.55em;
          }

         @media(max-width: 600px) {
            font-size: 1.4em;
          }        
        
        &.alt {
          font-size: 1.25em;
          
          @media(min-width: $breakpoint-md) {
            font-size: 0.7em;
          }
        }
      }
      
      form {
        margin-bottom: 0;
      }
      
      .input-slider {
        border: none;
        background: #ececec;
        color: #515151;
        height: 32px;
        min-height: 32px;
        margin-bottom: 0;
        font-size: .55em;
        @media(max-width: 470px) {
          font-size: 0.85em;
        }
      }
       @media(min-width: 1900px) {
             .input-slider {
            height: 42px;
            min-height: 42px;
            font-size: .75em;
              }
       }
      input[type="submit"] {
        font-size: 10px;
        width:100%;
        
         @media(min-width:1380px) {
            font-size: .45em;
          }
        @media(min-width: 1900px) {
          font-size:.75em;
        }
      }
    }    
  }
  

  @media (max-width: $breakpoint-md) {
    max-height: none;
    .gallery-cell {
      position: relative;
    }
    .banner-full-link {
      display: none !important;
    }
    .caption {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      margin: auto;
    }
    .caption-content {
      opacity: 1;
    }
  }
  /* ---------------------

  SLIDE SPECIFIC STYLES

  --------------------- */
  .slide-seamless-genius {
    .headline {
      font-size: 2.3rem;
      text-transform: none;
      font-weight: normal;
      @media (max-width: $breakpoint-sm-max) {
        font-size: 1.5rem;
      }
    }
    .post-head {
      font-size: 0.7rem;
    }
    .caption {
      @media (max-width: $breakpoint-sm-max) {
        top: 52%;
      }
    }
  }
  .slide-test-1 {
    @media (max-width: $breakpoint-sm-max) {
      .pre-head {
        position: relative;
        /*&:before {
          content: " ";
          width: 7rem;
          height: 0.1rem;
          background: transparent;
          border-left: 0.8rem solid white;
          border-right: 0.8rem solid white;
          position: absolute;
          left: 0;
          right: 0;
          top: 50%;
          margin: auto;
          -webkit-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          transform: translateY(-50%);
        }*/
      }
    }
  }
  @media (max-width: $breakpoint-sm-max) {
    .arrow, .flickity-prev-next-button {
      opacity: 1;
    }
    .flickity-prev-next-button svg {
      top: 47.5%;
    }
  }
}

img {
  max-width: 100%;
  &.img-fill-width {
    width: 100%;
  }
  &.img-bg {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    margin: auto;
    z-index: 0;
    width: 100%;
  }
}

div {
  &.content-home {
    overflow: hidden;
    @media (min-width: $breakpoint-lg) {
      .row.narrow {
        padding: 0 6.5rem;
      }
    }
    @media (min-width: $breakpoint-md) {
      .mimic-masonry {
        margin-top: -18.5rem;
        padding-right: 2.75rem !important;
      }
    }
    @media (max-width: $breakpoint-md-max) {
      .mimic-masonry img {
        width: 100%;
      }
    }
    @media (min-width: $breakpoint-sm) {
      .follow-left-img {
        max-width: 332px;
        margin: auto;
        padding-left: 0 !important;
      }
    }
    .row {
      margin-bottom: 0;
    }
  }
}

a {
  &.a-cat-block {
    position: relative;
    display: inline-block;
    p {
      text-decoration: none;
      color: #000;
      background: #fff;
      padding: 0.2rem 0rem 0;
      line-height: 1.8rem;
      display: inline-block;
      position: absolute;
      text-transform:uppercase;
      margin: auto;
      font-size: 0.6rem;
      letter-spacing: 0.05rem;
      min-width: 8.7rem;
      bottom: 0;
      width: auto;
      left: 50%;
      -webkit-transform: translateX(-50%);
      -ms-transform: translateX(-50%);
      transform: translateX(-50%);
      @media (max-width: $breakpoint-sm-max) {
        position: relative;
        max-width: 100%;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none;
        min-width: 0;
        left: 0;
        right: 0;
        margin: auto;
        line-height: 1.5rem;
        font-size: 0.5rem;
        display: inline-block;
        letter-spacing: 0.01rem;
        font-weight: bold;
        bottom: 1.5rem;
        width: auto;
        padding: 0 0.4rem;
      }
    }
    &:hover {
      p {
        text-decoration: none;
        color: $black;
      }
    }
  }
  &.a-btn-black {
    background: $black;
    display: inline-block;
    line-height: 1;
    color: $white;
    font-weight: 800;
    letter-spacing: 0;
    margin: 0;
    font-size: 0.4rem;
    padding: 0.4rem 0.5rem;
    min-width: 5.5rem;
    @media (min-width: $breakpoint-md) {
      font-size: 0.55rem;
      padding: 0.5rem 1rem 0.5rem 1rem;
      min-width: 7.8rem;
    }
    &:hover {
      color: $white;
      text-decoration: none;
    }
  }
  &.a-btn-white {
    background: $white;
    display: inline-block;
    line-height: 1;
    color: $black;
    font-weight: 800;
    letter-spacing: 0.05rem;
    margin: 0;
    font-size: 0.5rem;
    padding: 0.35rem;
    min-width: 5rem;
    @media (min-width: $breakpoint-md) {
      min-width: 7.8rem;
      font-size: 0.55rem;
      padding: 0.6rem 0.7rem 0.6rem 0.7rem;
    }
    &:hover {
      color: $black;
      text-decoration: none;
    }
  }
  &.a-underline {
    display: inline-block;
    color: $black;
    font-size: 0.5rem;
    font-weight: 700;
    border-bottom: 1px solid rgba(0, 0, 0, 0.12);
    line-height: 1;
    padding: 0;
    margin: 0;
    @media (min-width: $breakpoint-md) {
      font-size: 0.6rem; 
    }
    &:hover {
      color: $black;
      text-decoration: none;
    }
    &.text-white {
      color: $white;
      border-bottom-color: rgba(255, 255, 255, 0.3);
      &:hover {
        color: $white;
      }
    }
  }
}

h2 {
  &.serif {
    font-family: $serif_disp_pro_book;
    font-size: 1.4rem;
    font-weight: 400;
    text-transform: none;
    letter-spacing: 0;
    line-height: 1;
    margin: 0;
    padding: 0;
 &.fbpro {
     font-family:$serif_bigbook;
    }    
    @media (min-width: $breakpoint-md) {
      font-size: 2.4rem;
    }
    @media (max-width: $breakpoint-sm-max) {
      font-size: 1.5rem;
    }
  }
}

h3 {
  &.h3-grey {
    font-size: 0.7rem;
    letter-spacing: 0.05rem;
    text-align: center;
    font-weight: 100;
    position: relative;
    display: inline-block;
    margin: 0;
    line-height: 1;
    @media (min-width: $breakpoint-md) {
      font-size: 0.85rem;
      letter-spacing: 0.125rem;
    }
    &:before, &:after {
      content: " ";
      position: absolute;
      top: 0;
      bottom: 0;
      margin: auto;
      height: 0;
      border-bottom: 1px solid $grey-light;
      width: 1.2rem;
      @media (min-width: $breakpoint-md) {
        width: 2.1rem;
      }
    }
