.modal-size-chart {
  position: relative;
  background: white;
  padding: 4.2em 4.2em 6em;
  .tab-window {
    display: none;
    &.active {
      display: block;
    }
  }
  .tab-measure {
    position: relative;
    img {
      max-width: 100%;
      margin: 2em 0;
    }
  }
  .close {
    position: absolute;
    right: 0;
    top: 0;
    width: 2.5em;
    height: 2.5em;
    background: white;
    padding: 0;
    margin: 0;
    min-height: 0;
    border: 0;
    margin-right: 1.5em;
    margin-top: 1.5em;
    cursor: pointer;
    &:before, &:after {
      content: " ";
      width: 1px;
      height: 100%;
      -webkit-transform: rotateZ(45deg);
      -ms-transform: rotate(45deg);
      transform: rotateZ(45deg);
      border-right: 1px solid #949494;
      position: absolute;
      top: 0;
      right: 50%;
