/* #Home page - featured promotions
================================================== */

.featured-promotions {
  .feature-section {
    text-align: center;
    float: left;
    margin: 0 10px 10px;
    position: relative;
    @include respond-to('medium'){
      opacity: 1;
    }
    {% if settings.featured_links_style != blank %}
      opacity: 0;
    {% endif %}
    .link-promo {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      display: block;
    }
    .feature-inner {
      display: block;
      position: relative;
    }
    .feature-overlay {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-color: rgba(0,0,0,0);
      transition: background-color 0.25s linear;
      @include respond-to('medium'){
        background-color: rgba(0,0,0,0.5);
      }
    }
    .feature-wrap {
      @include vertical-alignment;
      position: absolute;
      width: 80%;
      margin: 0 auto;
      left: 0;
      right: 0;
      pointer-events: none;
    }
    .feature-details {
      display: none;
      opacity: 0;
      transition: opacity 0.3s linear;
      @include respond-to('medium'){
        opacity: 1;
        display: block !important;
      }
      &.reveal-details {
        opacity: 1;
      }
      .button {
        margin-top: 15px;
      }
    }
    .description {
      color: {{settings.featured_promos_text_color}};
      margin-bottom: 0;
    }
    .description a {
      pointer-events: auto;
    }
    &.title-overlay-true {
      .title {
        transition: all 0.3s linear;
        color: {{settings.featured_promos_text_color}};
        @include respond-to('medium'){
          margin-bottom: 10px;
        }
        &:after {
          content: '';
          display: block;
          {% if settings.headline_border == "thick" %}
            height: 2px;
          {% else %}
            height: 1px;
          {% endif %}
          width: 0;
          background: transparent;
          transition: width .5s ease, background-color .5s ease;
          margin: 10px auto auto auto;
          @include respond-to('medium'){
            display: none;
          }
        }
      }
    }
    &.title-overlay-false {
      .feature-wrap .title {
        display: none;
      }
    }
    &:hover .feature-overlay {
      background-color: rgba(0,0,0,0.5);
    }
    &.title-overlay-true:hover .title:after {
      width: 50%;
      background: #FFF;
    }
    img {
      width: 100%;
      display: block;
    }
  }
}

$i: 1;
$delay: 0;
@while $i < 8 {
  .promo-#{$i} {
    -webkit-animation-delay: $delay + s;
    animation-delay: $delay + s;
  }
  $i: $i + 1;
  $delay: $delay + 0.3;
}

.featured-promotions.promo-per-row-2 {
  .feature-section {
    width: calc(50% - 20px);
    @include respond-to('medium'){
      width: 100%;
      margin: 0 0 10px;
    }
    &:nth-child(2n+1){
      clear: both;
    }
  }
}

.featured-promotions.promo-per-row-3 {
  .feature-section {
    width: calc(33.3333% - 20px);
    @include respond-to('medium'){
      width: 100%;
      margin: 0 0 10px;
    }
    &:nth-child(3n+1){
      clear: both;
    }
  }
}

.featured-promotions.promo-per-row-4 {
