/* Promo banner */

$promoBannerHeight: 30px;

{% if settings.promo_text != blank %}
  .promo_banner {
    background-color: {{ settings.promo_bg_color }};
    text-align: center;
    color: {{ settings.promo_text_color }};
    font-size: 12px;
    position: relative;
    width: 100%;
    height: 0;
    line-height: 0;
    z-index: 5000;
    top: 0;
    transition: all 0.3s linear;
    overflow: hidden;
    .promo_banner-close {
      position: absolute;
      top: 0;
      right: 0;
      display: block;
      overflow: visible;
      width: 35px;
      height: 35px;
      margin: 0;
      padding: 0;
      cursor: pointer;
      text-decoration: none;
      color: {{ settings.promo_text_color }};
      border: 0;
      outline: 0;
      background: transparent;
      z-index: 1000;
      &:before {
        font-family: Arial, "Helvetica CY", "Nimbus Sans L", sans-serif !important;
        font-size: 20px;
        line-height: $promoBannerHeight;
        position: absolute;
        top: 0;
        left: 0;
        display: block;
        width: 35px;
        content: "\00d7";
        text-align: center;
      }
    }
  }

  .promo_banner-show .promo_banner {
    transition: all 0s linear;
    height: $promoBannerHeight;
    line-height: $promoBannerHeight;
  }

  .top_bar {
    padding-top: 0;
    position: relative;
    transition: padding-top 0.3s linear;
    @include respond-to('small'){
      padding-top: 0;
    }
  }

{% endif %}

$topMenuHeight: 40px;

{% if settings.header_background == false %}

  #pagecontent.behind-menu-true {
    display: block;
    padding-bottom: $topMenuHeight;
  }

  .promo_banner-show {
    #pagecontent.behind-menu-true {
      display: block;
      padding-bottom: calc(#{$topMenuHeight} + #{$promoBannerHeight});
    }
  }

{% endif %}

@include respond-to('medium'){

  #pagecontent.behind-menu-true {
    display: block;
    padding-bottom: $topMenuHeight;
  }

  .promo_banner-show {
    #pagecontent.behind-menu-true {
      display: block;
      padding-bottom: calc(#{$topMenuHeight} + #{$promoBannerHeight});
    }
  }

  #pagecontent.behind-menu-false {
    display: block;
    padding-bottom: $topMenuHeight;
  }

  div.content {
    padding-top: calc(#{$topMenuHeight} + 30px);
  }

  .page_banner {
    margin-top: $topMenuHeight;
  }

  .promo_banner-show {
    #pagecontent.behind-menu-false {
      display: block;
      padding-bottom: calc(#{$topMenuHeight} + #{$promoBannerHeight});
    }
    div.content {
      padding-top: calc(#{$topMenuHeight} + #{$promoBannerHeight} + 30px);
    }
    .page_banner {
      margin-top: calc(#{$topMenuHeight} + #{$promoBannerHeight});
    }
  }

}

